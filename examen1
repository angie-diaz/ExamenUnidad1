for(num<-Range(1,10)){
if(num%2==0){
println($"num primo")
}else{
println($"num impar")
}}

 //1. par o no es par
for(num <- Range(0,10)){
if(num%2 == 0){
println(s"$num is par")
}else{ println(s"$num is no es par")}}

//2.funcion que devuelva true o false
def primo(num:Int): Boolean={
for(n<-Range(2,num)){
if(num%n == 0){
return false
}}
return true
}

